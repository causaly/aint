{
  "env": {
    "targets": {
      "node": "14"
    },
    "mode": "entry",
    "coreJs": 3
  },
  "test": [".*.ts"],
  "exclude": [".*.spec.ts", ".*.test.ts"],
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "dynamicImport": false,
      "decorators": false
    },
    "loose": false,
    "externalHelpers": true
  },
  "minify": false,
  "module": {
    "type": "commonjs",
    "strict": false,
    "strictMode": true,
    "lazy": false,
    "noInterop": false,
    "ignoreDynamic": false
  }
}
